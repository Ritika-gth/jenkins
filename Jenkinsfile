pipeline {
  agent any
  stages {
    stage ('Clone Repo') {
      steps {
        echo 'Cloned from repo github.com/Ritika-gth/jenkins.com'
      }
    } 
    stage ('Build') {
      steps {
        echo 'Building project...'
      }
    }
    stage ('Test') {
      steps {
        echo 'Testing project...'
      }
    }
    stage ('Deploy') {
      steps {
        echo 'Deploying project...'
      }
    }
  }
}
